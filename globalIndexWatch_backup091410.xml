function getInitCaps__MODULE_ID__(nt) {
    var sarr = nt.split(" ");
    var r = "";
    for (i = 0; i < sarr.length; i++) {
        var s = sarr[i].toLowerCase();
        for (j = 0; j < s.length; j++) {
            if (j == 0) {
                r += s.charAt(j).toUpperCase();
            } else {
                r += s.charAt(j);
            }
        }
        r += " ";
    }
    r = r.substring(0, r.length - 1);
    return r;
}


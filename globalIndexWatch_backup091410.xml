		} else {
			try {
				curQuoteChange = parseFloat(curQuoteChange);
				curQuoteChange = roundNumber__MODULE_ID__(curQuoteChange);
				curQuoteChange = round2Decimals__MODULE_ID__(curQuoteChange);
			} catch (e) {

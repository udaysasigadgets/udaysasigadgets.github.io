    var stA = new Array();
    for (var l = 0; l < arr.length; l++) {
        stA.push(arr[l].symbol);
    }
    stA.sort();
    for (var h = 0; h < stA.length; h++) {
        for (var o = 0; o < arr.length; o++) {
            if (stA[h] == arr[o].symbol) {
                newArray.push(arr[o]);
            }
        }
    }
    return newArray;
}

